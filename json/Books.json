__yaml__:

  - &uugra
    caption:      caption
    rowkeep: 1
    uuid:         uugra
    desc:         __desc__
    dreftymacid:  __dreftymacid__
    datainclude:      datainclude
    templateinclude:  templateinclude
    template:         |
        {% for item in odata -%}
        {{-"\n"-}}        
        - &dXVjdml{{ loop.index | jjfmt('{:0>3}') }}      
          bookuuid: "dXVjdml{{ loop.index | jjfmt('{:0>3}') }}"
          bookindx: {{ loop.index | jjfmt('{:0>3}') |jjhug }}
          booktitl: {{ item |jjhug }}
        {% endfor %}
        

